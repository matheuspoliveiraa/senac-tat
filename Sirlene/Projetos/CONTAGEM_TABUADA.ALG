Algoritmo "Contagem_tabuada"

Var
   cont: inteiro
   num, tab: real


Inicio

      escreval ("*** TABUADA ***")
      escreval()
      escreval ("Digite um número: ")
      leia (num)

   cont<-0
   enquanto (cont<=10) faca
   
   tab <- num*cont
   escreval (num, " x" , cont, " =", tab)
         cont <- cont + 1
   fimenquanto




Fimalgoritmo